{
  "name": "playingThree",
  "version": "0.0.1",
  "description": "A test playground for testing three js",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets=env --plugins=add-module-exports,transform-runtime",
    "babel-debug": "babel-node --inspect --presets=env --plugins=add-module-exports,transform-runtime",
    "play": "cross-env nodemon --exec npm run babel-node -- index",
    "debug": "cross-env nodemon --exec npm run babel-debug -- index"
  },
  "dependencies": {
    "nodemon": "^1.11.0",
    "winston": "^2.3.0",
    "lodash": "^4.17.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "moment": "^2.19.0",
    "cross-env": "^5.1.1",
    "three": "0.91.0",
    "express": "^4.14.0",
    "config": "^1.24.0",
    "canvas": "1.6.10"
  }
}
